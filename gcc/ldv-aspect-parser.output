Grammar

    0 $accept: input $end

    1 input: /* empty */
    2      | input advice_definition
    3      | input named_pointcut

    4 named_pointcut: LDV_ID LDV_ID ':' composite_pointcut

    5 advice_definition: advice_declaration LDV_BODY

    6 advice_declaration: LDV_ID ':' composite_pointcut

    7 composite_pointcut: LDV_ID
    8                   | primitive_pointcut
    9                   | '!' composite_pointcut
   10                   | composite_pointcut "||" composite_pointcut
   11                   | composite_pointcut "&&" composite_pointcut
   12                   | '(' composite_pointcut ')'

   13 primitive_pointcut: LDV_MACRO_POINTCUT '(' primitive_pointcut_signature_macro ')'
   14                   | LDV_FILE_POINTCUT '(' primitive_pointcut_signature_file ')'
   15                   | LDV_ID '(' primitive_pointcut_signature_declaration ')'

   16 primitive_pointcut_signature_macro: LDV_ID
   17                                   | LDV_ID '(' macro_param_opt ')'

   18 primitive_pointcut_signature_file: LDV_FILE

   19 macro_param_opt: /* empty */
   20                | macro_param

   21 macro_param: LDV_ID
   22            | macro_param ',' LDV_ID
   23            | "..."
   24            | LDV_ID "..."
   25            | macro_param ',' "..."
   26            | macro_param ',' LDV_ID "..."

   27 primitive_pointcut_signature_declaration: c_declaration

   28 c_declaration: c_declaration_specifiers
   29              | c_declaration_specifiers c_declarator

   30 $@1: /* empty */

   31 $@2: /* empty */

   32 c_declaration_specifiers: $@1 c_declaration_specifiers_aux $@2

   33 c_declaration_specifiers_aux: c_storage_class_specifier c_declaration_specifiers_opt
   34                             | c_type_specifier c_declaration_specifiers_opt
   35                             | c_type_qualifier c_declaration_specifiers_opt
   36                             | c_function_specifier c_declaration_specifiers_opt
   37                             | ".."
   38                             | "..."

   39 c_declaration_specifiers_opt: /* empty */
   40                             | c_declaration_specifiers_aux

   41 c_storage_class_specifier: LDV_TYPEDEF
   42                          | LDV_EXTERN
   43                          | LDV_STATIC
   44                          | LDV_AUTO
   45                          | LDV_REGISTER

   46 c_type_specifier: LDV_VOID
   47                 | LDV_CHAR
   48                 | LDV_INT
   49                 | LDV_FLOAT
   50                 | LDV_DOUBLE
   51                 | LDV_BOOL
   52                 | LDV_COMPLEX
   53                 | LDV_SHORT
   54                 | LDV_LONG
   55                 | LDV_SIGNED
   56                 | LDV_UNSIGNED
   57                 | LDV_STRUCT LDV_ID
   58                 | LDV_UNION LDV_ID
   59                 | LDV_ENUM LDV_ID
   60                 | LDV_TYPEDEF_NAME
   61                 | LDV_UNIVERSAL_TYPE_SPECIFIER

   62 c_type_qualifier: LDV_CONST
   63                 | LDV_RESTRICT
   64                 | LDV_VOLATILE

   65 c_function_specifier: LDV_INLINE

   66 c_declarator: c_pointer_opt c_direct_declarator

   67 c_direct_declarator: LDV_ID
   68                    | '(' c_declarator ')'
   69                    | c_direct_declarator '[' int_opt ']'
   70                    | c_direct_declarator '(' c_parameter_type_list ')'

   71 c_pointer_opt: /* empty */
   72              | c_pointer

   73 c_pointer: '*' c_type_qualifier_list_opt
   74          | '*' c_type_qualifier_list_opt c_pointer

   75 int_opt: /* empty */
   76        | LDV_INT_NUMB
   77        | LDV_ID

   78 c_type_qualifier_list_opt: /* empty */
   79                          | c_type_qualifier_list

   80 c_type_qualifier_list: c_type_qualifier
   81                      | c_type_qualifier_list c_type_qualifier

   82 c_parameter_type_list: c_parameter_list

   83 c_parameter_list: c_parameter_declaration
   84                 | c_parameter_list ',' c_parameter_declaration

   85 c_parameter_declaration: c_declaration_specifiers c_declarator
   86                        | c_declaration_specifiers c_abstract_declarator_opt

   87 c_abstract_declarator: c_pointer
   88                      | c_pointer_opt c_direct_abstract_declarator

   89 c_abstract_declarator_opt: /* empty */
   90                          | c_abstract_declarator

   91 c_direct_abstract_declarator: '(' c_abstract_declarator ')'
   92                             | c_direct_abstract_declarator_opt '[' int_opt ']'
   93                             | c_direct_abstract_declarator '(' c_parameter_type_list_opt ')'

   94 c_direct_abstract_declarator_opt: /* empty */
   95                                 | c_direct_abstract_declarator

   96 c_parameter_type_list_opt: /* empty */
   97                          | c_parameter_type_list


